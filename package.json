{
  "name": "automated-software-testing-with-playwright",
  "version": "1.0.0",
  "description": "Automated software testing with Playwright",
  "main": "index.js",
  "scripts": {
    "tests:chrome": "playwright test --config=playwright.config.ts --project=Chromium",
    "tests:firefox": "playwright test --config=playwright.config.ts --project=Firefox",
    "tests:webkit": "playwright test --config=playwright.config.ts --project=Webkit",
    "tests:e2e": "playwright test --config=e2e.config.ts --project=Webkit",
    "tests:visual": "playwright test --config=visual.config.ts --project=Webkit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Anastasiia-web/Automated-software-testing-with-playwright.git"
  },
  "author": "Anastasiia Ivashchenko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Anastasiia-web/AutomatedTestingWithPlaywright/issues"
  },
  "homepage": "https://github.com/Anastasiia-web/AutomatedTestingWithPlaywright#readme",
  "dependencies": {
    "@playwright/test": "^1.27.1",
    "prettier": "^2.7.1"
  }
}